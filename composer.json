{
    "name": "choval/async",
    "description": "A set of functions to help coding in async",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Osvaldo Jiang",
            "homepage": "https://github.com/choval"
        }
    ],
    "autoload": {
        "psr-4": {
            "Choval\\Async\\": "src/"
        },
        "files": [
            "src/functions.php"
        ]
    },
    "require": {
        "php": "^7.2|^8.0|^8.1",
        "react/promise-stream": "^1.3",
        "react/child-process": "^0.6",
        "react/promise": "^2.8|^2.9",
        "react/event-loop": "^1.2|^1.3"
    },
    "suggest": {
        "ext-pcntl": "Required to create childs to async blocking code",
        "ext-posix": "Required to kill hanging processes",
        "ext-sockets": "Required to create a channel for communication with child processes"
    },
    "require-dev": {
        "phpunit/phpunit": "^9"
    }
}
